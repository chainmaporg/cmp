    <!-- header part start -->
    <% include ./partials/header %>
    <!-- header part end -->
    <!-- body part start -->

    <body>

        <div class="loader">
            <div class="loader_html"></div>
        </div>

        <div id="wrap" class="grid_1200">

            <% include ./partials/header_login %>
            <% include ./partials/body_header %>
            <div class="section-warp ask-me">
                <div class="container clearfix">
                    <div class="box_icon box_warp box_no_border box_no_background" box_border="transparent" box_background="transparent" box_color="#FFF">
                        <div class="row">
                            <div class="col-md-12">
                                <h2>Map of Blockchain Solutions</h2>
                                <div class="clearfix"></div>
                            </div>

                        </div>
                        <!-- End row -->
                    </div>
                    <!-- End box_icon -->
                </div>
                <!-- End container -->
            </div>
            <!-- End section-warp -->

            <section class="container main-content">
            
            <%
               for(var i=0; i<solutions.length; i++) {
               	console.log("......",solutions[i]);
               	imgname="/image/solution-"+solutions[i]+".jpg";
               	steps=data[solutions[i]];
               	console.log("......steps:",steps);
               	
            %>
                        <div class="row">
                        
                            <!--<div class="boxedtitle page-title">-->
                                <table><tr><td> <img src=<%=imgname%> width="300" height="300"/> 
                                        </td>
                                         </tr>
                                </table>
                            <!--</div>-->
					        <br>
                            <div class="col-md-12">
                                <table>
                                <% 
                                 for(var k = 0;k < steps.length;k++) { %>

                                 <tr>
                                   <td width="300">
                                   		<h3>Step <%=steps[k].step+" - "+steps[k].step_name%></h3>
                                   </td>
                                   <td width="500">
                                   		<%=steps[k].step_description%>
                                   </td>
                                   <td width="200">
                                   		<a href=<%=steps[k].step_url%>>Read the article</a>
                                   </td>
                                 </tr>

                                 <% } %>
								 </table>
								 
	                        </div> <!--col-md-12-->
                        </div> <!--row-->
                        
           <% } %>      
 
            </section>



            <!-- body footer start -->
            <% include ./partials/body_footer %>
            <!-- body footer end -->
        </div>
        <!-- End wrap -->

        <!-- footer started -->
        <% include ./partials/footer_js %>
        <!-- footer end -->
        
                 

